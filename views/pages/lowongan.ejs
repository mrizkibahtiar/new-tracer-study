<body>
    <nav id="topnav" class="defaultscroll is-sticky">
        <div class="container relative">
            <!-- Start Logo container-->
            <a class="logo" href="index.html">
                <span class="inline-block dark:hidden">
                    <img src="assets/landing/images/logo-dark.png" class="l-dark" width="170">
                    <img src="assets/landing/images/logo-light.png" class="l-light" width="170">
                </span>
                <img src="assets/landing/images/logo-light.png" width="170" class="hidden dark:inline-block" alt="">
            </a>
            <!-- End Logo container-->

            <!-- Start Mobile Toggle -->
            <div class="menu-extras">
                <div class="menu-item">
                    <a class="navbar-toggle" id="isToggle" onclick="toggleMenu()">
                        <div class="lines">
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </a>
                </div>
            </div>
            <!-- End Mobile Toggle -->

            <!--Login button Start-->
            <ul class="buy-button list-none mb-0">
                <li class="sm:inline ps-1 mb-0 hidden">
                    <a href="/loginPage"
                        class="btn bg-green-600 hover:bg-green-700 border-green-600 dark:border-green-600 text-white !rounded-md"
                        style="font-size: 1.5rem;">Masuk</a>
                </li>
            </ul>
            <!--Login button End-->

            <div id="navigation">
                <!-- Navigation Menu-->
                <ul class="navigation-menu !justify-end nav-light">
                    <li><a href="/" class="sub-menu-item" style="font-size: 1.5rem;">Beranda</a></li>

                    <li><a href="/berita" class="sub-menu-item" style="font-size: 1.5rem;">Berita</a></li>
                    <li><a href="/lowongan" class="sub-menu-item" style="font-size: 1.5rem;">Lowongan</a></li>
                    <li><a href="/saran" class="sub-menu-item" style="font-size: 1.5rem;">Saran</a></li>
                </ul><!--end navigation menu-->
            </div><!--end navigation-->
        </div><!--end container-->
    </nav>



    <section class="relative table w-full py-32 lg:py-36"
        style="background-image: url('assets/landing/images/bg-lowongan.jpg'); background-size: cover; background-position: center 60%;">
        <div class="absolute inset-0 bg-slate-900 opacity-80"></div>
        <div class="container relative">
            <div class="grid grid-cols-1 text-center mt-2">
                <h3 class="md:text-4xl text-3xl md:leading-normal leading-normal font-medium text-white">Informasi
                    Lowongan
                </h3>
            </div><!--end grid-->
        </div><!--end container-->
    </section>
    <div class="relative">
        <div class="shape overflow-hidden z-1 text-white dark:text-slate-900">
            <svg viewBox="0 0 2880 48" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M0 48H1437.5H2880V0H2160C1442.5 52 720 0 720 0H0V48Z" fill="currentColor"></path>
            </svg>
        </div>
    </div>
    <!-- Start -->
    <section class="relative lg:py-24 py-16">
        <div class="container relative">
            <% if(lowonganList.length> 0) { %>
                <div class="grid lg:grid-cols-3 md:grid-cols-2 grid-cols-1 gap-[30px]">
                    <% lowonganList.forEach(function(item) { %>

                        <div
                            class="group rounded-xl bg-white dark:bg-slate-900 shadow-sm hover:shadow-xl dark:hover:shadow-xl dark:shadow-gray-700 overflow-hidden ease-in-out duration-500">
                            <a href="/lowongan/<%= item._id %>">
                                <div class="relative">
                                    <img src="/uploads/<%= item.featuredImage %>" alt="<%= item.title %>"
                                        style="height: 200px; object-fit: cover; width: 100%;">
                                </div>
                                <div class="p-6">
                                    <div style="padding-bottom: 0.75rem;">
                                        <a href="/lowongan/<%= item._id %>"
                                            class="text-2xl hover:text-green-600 font-medium ease-in-out duration-500 title-link"
                                            style="height: 4rem; overflow: hidden; display: -webkit-box; -webkit-box-orient: vertical;">
                                            <%= item.title %>
                                        </a>
                                        <p class="text-sm text-gray-500 mt-2">
                                            <%= item.createdAt.toLocaleDateString('id-ID', { day: 'numeric' ,
                                                month: 'long' , year: 'numeric' }) %>
                                        </p>
                                    </div>
                                    <div class="pt-3 ">
                                        <p
                                            style="overflow: hidden; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 3;">
                                            <%= item.content.substring(0, 150) + '...' %>
                                        </p>
                                    </div>
                                </div>
                            </a>
                        </div>


                        <% }); %>
                </div>
                <% } else { %>
                    <div style="margin-top: 1.5rem;">
                        <div>
                            <div style="padding: 1.5rem;">
                                <h1 style="color: #000; text-align: center; font-weight: bold; font-size: 1.5rem;">
                                    Belum Ada Lowongan
                                </h1>
                            </div>
                        </div>
                    </div>
                    <% } %>
        </div><!--end container-->
    </section><!--end section-->

    <footer class="relative bg-slate-900 dark:bg-slate-800 mt-24">
        <div class="container relative">
            <div class="relative py-16">
                <!-- Subscribe -->
